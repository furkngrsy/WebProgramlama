@using Entity.Concrete
@model List<Kategori>
@{
    ViewData["Title"] = "Index";
}
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<table class="table table-bordered">
    <tr>
        <th>Kategori ID</th>
        <th>Kategori Adı</th>
        <th>Durumu</th>
        <th>Detaylar</th>
        <th>Sil</th>
    </tr>
    @foreach(var item in Model)
            {
<tr>
    <td>@item.KategoriID</td>
    <td>@item.KategoriAd</td>
    <td>@item.KategoriDurum</td>
    <td>@item.KategoriAciklama</td>
    <td><a href="#" class="btn btn-warning">Detaylar</a></td>
    <td><a href="#" class="btn btn-warning">Sil</a></td>
</tr>
            }
</table>

